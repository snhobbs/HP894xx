{\fonttbl
{\f0\fnil Helv;}
}
{\plain {These two variables can be adjusted.}}
}
.EQN 2 0 0 0
{0:\a}NAME:.35
.EQN 0 10 0 0
{0:widthInSymbols}NAME:20
.TXT 4 -10 0 0
Cg a35.000000,73.000000,41
{\rtf1\ansi \deff0
{\fonttbl
{\f0\fnil Helv;}
}
{\plain {These variables should not be changed.}}
}
.EQN 3 0 0 0
{0:ptsPerSymbol}NAME:20
.EQN 0 17 0 0
{0:T}NAME:1
.EQN 4 -17 0 0
{0:numPts}NAME:{0:ptsPerSymbol}NAME*{0:widthInSymbols}NAME+1
