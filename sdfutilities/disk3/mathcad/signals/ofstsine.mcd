Cg a72.625000,73.000000,162
{\rtf1\ansi \deff1
{\fonttbl
{\f0\fnil Helv;}
{\f1\fnil Arial;}
}
{\plain {This Mathcad file creates a complex
 time waveform for use in the HP894x0A source.
  This example creates a sinewave that is
 offset from the center frequency.\par }}
}
.EQN 7 0 0 0
{0:f.offset}NAME:1000
.EQN 5 0 0 0
{0:F.samp}NAME:1.28*(10)^(5)
.TXT 0 18 0 0
Cg a49.375000,55.000000,98
{\rtf1\ansi \deff1
{\fonttbl
{\f0\fnil Helv;}
{\f1\fnil Arial;}
}
{\plain {The sampling frequency should be
 at least 1.28 time the\par bandwidth of
 the signal being created.}}
}
.EQN 7 -18 0 0
{0:dt}NAME:(1)/({0:F.samp}NAME)
.EQN 0 11 0 0
{0:dt}NAME={18999}?_n_u_l_l_
.TXT 0 15 0 0
Cg a46.375000,47.000000,66
{\rtf1\ansi \deff1
{\fonttbl
{\f0\fnil Helv;}
{\f1\fnil Arial;}
}
{\plain {This number will be used later 
 in the ASCII to SDF conversion}}
}
.EQN 6 -26 0 0
{0:numPts}NAME:2048
.EQN 4 0 0 0
{0:n}NAME:0;{0:numPts}NAME-1
.TXT 1 50 0 0
Cg a19.625000,23.000000,52
{\rtf1\ansi \deff1
{\fonttbl
{\f0\fnil Helv;}
{\f1\fnil Arial;}
}
{\plain {\fs20 Aug }{\fs20 29, 1993\par 
signals\\ofstsine.mcd}}
}
.EQN 4 -50 0 0
({0:\q}NAME)[({0:n}NAME):2*{0:\p}NAME*{0:f.offset}NAME*({0:n}NAME*{0:dt}NAME)
.TXT 5 0 0 0
Cg a30.875000,73.000000,39
{\rtf1\ansi \deff1
{\fonttbl
{\f0\fnil Helv;}
{\f1\fnil Arial;}
}
{\plain {Create the real part of the waveform}}
}
.EQN 3 0 0 0
({0:x}NAME)[({0:n}NAME,0):{0:cos}NAME(({0:\q}NAME)[({0:n}NAME))
.TXT 6 0 0 0
Cg a63.875000,73.000000,46
{\rtf1\ansi \deff1
{\fonttbl
{\f0\fnil Helv;}
{\f1\fnil Arial;}
}
{\plain {Create the imaginary part of the
 waveform. }}
}
.EQN 3 0 0 0
({0:x}NAME)[({0:n}NAME,1):{0:sin}NAME(({0:\q}NAME)[({0:n}NAME))
.EQN 4 1 0 0
&&({0:x}NAME)[({0:n}NAME,0),({0:x}NAME)[({0:n}NAME,1)@&&{0:n}NAME
